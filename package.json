{
  "name": "pinbook",
  "version": "0.0.1",
  "description": "A simple Bookmark App",
  "main": "server.js",
  "repository": "git@github.com:eschocolat/pinbook.git",
  "author": "eschocolat",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && cross-env NODE_ENV=production node_modules/.bin/babel-node --presets react,es2015 app/server.js",
    "build": "cross-env NODE_ENV=production node_modules/.bin/webpack -p",
    "dev": "npm run dev:react & npm run dev:server",
    "dev:react": "cross-env NODE_ENV=development node_modules/.bin/webpack -w",
    "dev:server": "cross-env NODE_ENV=development node_modules/.bin/nodemon app/server.js --exec node_modules/.bin/babel-node --presets react,es2015"
  },
  "devDependencies": {
    "nodemon": "^1.11.0",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.1",
    "cross-env": "^3.2.3",
    "express": "^4.15.2",
    "express-handlebars": "^3.0.0",
    "jsonwebtoken": "^7.3.0",
    "moment": "^2.17.1",
    "mongoose": "^4.9.0",
    "morgan": "^1.8.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2"
  }
}
